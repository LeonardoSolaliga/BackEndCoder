<div>
    <% if (products.length){%>
        <table>
            <tr style="border: 1px solid black">
                <th style="border: 1px solid black">title</th>
                <th style="border: 1px solid black">price</th>
                <th style="border: 1px solid black">thumbnail</th>
            </tr>
            <% products.forEach(function(producto) {%>
                <tr style="border: 1px solid black">
                    <td style="border: 1px solid black"><%= producto.title %></td>
                    <td style="border: 1px solid black"><%= producto.price %></td>
                    <td style="border: 1px solid black"><%= producto.thumbnail %></td>
                </tr>
            <% })%>
        </table>
    <% } %>
    <% if (!products.length){%>
        <h4>No hay productos</h4>
    <% } %>
</div>